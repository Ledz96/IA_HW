<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### CENTRALIZED AGENTS ### -->
	<agent name="centralized-naive">
		<set class-path="bin/"/>
		<set class-name="template.CentralizedTemplate"/>
		<set algorithm="naive"/>
	</agent>

	<agent name="centralized-main">
		<set class-path="bin/"/>
		<set class-name="template.CentralizedTemplate"/>
		<set algorithm="SLS"/>
		<set iterations="20000"/>
		<set stuck-limit="2000"/>
		<set explore-prob="0.3"/>
	</agent>
	
</agents>
